from collections import deque

rows, columns = map(int, input().split())
text = deque(input())

matrix = [[''] * columns for _ in range(rows)]

for row in range(rows):
    if row % 2 == 0:
        for col in range(columns):
            matrix[row][col] = text[0]
            text.rotate(-1)
    else:
        for col in range(columns - 1, -1, -1):
            matrix[row][col] = text[0]
            text.rotate(-1)

for row in matrix:
    print(''.join(row))
